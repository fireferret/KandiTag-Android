<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    <ImageView
        android:id="@+id/UsersAndGroupsForNewMessageListItem_ImageView"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_width="70dp"
        android:layout_height="70dp" />

    <TextView
        android:paddingTop="26dp"
        android:paddingLeft="5dp"
        android:textColor="@color/white"
        android:id="@+id/UsersAndGroupsForNewMessageListItem_Username"
        android:layout_toRightOf="@+id/UsersAndGroupsForNewMessageListItem_ImageView"
        android:layout_width="fill_parent"
        android:layout_height="70dp" />

    <TextView
        android:paddingTop="26dp"
        android:paddingLeft="5dp"
        android:textColor="@color/white"
        android:id="@+id/UsersAndGroupsForNewMessageListItem_KandiName"
        android:layout_toRightOf="@+id/UsersAndGroupsForNewMessageListItem_ImageView"
        android:layout_width="fill_parent"
        android:layout_height="70dp" />
    
    <TextView
        android:id="@+id/UsersAndGroupsForNewMessageListItem_kandiId"
        android:layout_width="0dp"
        android:layout_height="0dp" />

    <TextView
        android:id="@+id/UsersAndGroupsForNewMessageListItem_ktid"
        android:layout_width="0dp"
        android:layout_height="0dp" />

</RelativeLayout>