<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    <ImageView
        android:id="@+id/GroupsForNewMessageListItem_ImageView"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_width="70dp"
        android:layout_height="70dp" />

    <TextView
        android:paddingTop="26dp"
        android:paddingLeft="5dp"
        android:id="@+id/GroupsForNewMessageListItem_KandiName"
        android:layout_toRightOf="@+id/GroupsForNewMessageListItem_ImageView"
        android:layout_width="fill_parent"
        android:layout_height="70dp" />
    
    <TextView
        android:id="@+id/GroupsForNewMessageListItem_qrCode"
        android:layout_width="0dp"
        android:layout_height="0dp" />

    <TextView
        android:id="@+id/usersGroupsForNewMessageListItem_ktid"
        android:layout_width="0dp"
        android:layout_height="0dp" />

</RelativeLayout>